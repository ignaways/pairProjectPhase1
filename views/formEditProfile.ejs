<%- include('header') %> 
    <form action="/profile" method="post">
      <% result.forEach(el => { %>
        <label for="name">Name:</label><br />
        <input type="text" id="name" name="name" value="<%=el.name%>" /><br />
        <label for="address">Address:</label><br />
        <input type="text" id="address" name="address" value="" /><br /><br />
        <label for="gender">Gender:</label><br />
        <input type="text" id="gender" name="gender" value="" /><br />
        <label for="phoneNumber">Phone Number:</label><br />
        <input type="text" id="lname" name="lname" value="" /><br /><br />
        <label for="email">Email:</label><br />
        <input type="text" id="email" name="email" value="" /><br />
        <label for="birthdayDate">Birthday:</label>
        <input type="date" id="birthdayDate" name="birthdayDate" />
        <input type="submit" value="Submit" />
      <% }) %>
    </form>
<%- include('footer') %> 

